    <snippet attachedevent="false" class="scripts.Design_17_17_FireBullet" classname="scripts.Design_17_17_FireBullet" desc="Spawn bullets from the actor's position." description="Spawn bullets from the actor's position." design="true" drawOrder="0" id="17" name="Fire Bullet" oid="-1" package="scripts" shared="false" type="actor">
